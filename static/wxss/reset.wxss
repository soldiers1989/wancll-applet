/* 12栅格 */

.wancll-col-xs-1, .wancll-col-xs-2, .wancll-col-xs-3, .wancll-col-xs-4,
.wancll-col-xs-5, .wancll-col-xs-6, .wancll-col-xs-7, .wancll-col-xs-8,
.wancll-col-xs-9, .wancll-col-xs-10, .wancll-col-xs-11, .wancll-col-5 {
  position: relative;
  /* float: left; */
}

.wancll-col-xs-12 {
  width: 100%;
  position: relative;
}

.wancll-col-xs-11 {
  width: 91.6666667%;
}

.wancll-col-xs-10 {
  width: 83.3333333%;
}

.wancll-col-xs-9 {
  width: 75%;
}

.wancll-col-xs-8 {
  width: 66.6666667%;
}

.wancll-col-xs-7 {
  width: 58.3333333%;
}

.wancll-col-xs-6 {
  width: 50%;
}

.wancll-col-xs-5 {
  width: 41.6666667%;
}

.wancll-col-xs-4 {
  width: 33.3333333%;
}

.wancll-col-xs-3 {
  width: 25%;
}

.wancll-col-xs-2 {
  width: 16.6666667%;
}

.wancll-col-xs-1 {
  width: 8.33333333%;
}

.wancll-col-5 {
  width: 20%;
}

/* 宫格布局 */

.wancll-grid {
  width: 100%;
  display: table;
  table-layout: fixed;
  background-color: #fff;
}

.wancll-grid [class*=aui-col-] {
  display: table-cell;
  position: relative;
  text-align: center;
  vertical-align: middle;
}

/* 去浮动 */

.clearfix:after {
  clear: both;
  display: block;
  content: "";
}

/* 浮动 */

.wancll-fl {
  float: left;
}

.wancll-fr {
  float: right;
}

/* 弹性布局 */

.wancll-flex {
  display: flex;
  align-items: center;
}

.wancll-borderbox {
  box-sizing: border-box;
}

/* form */

.form input {
  height: 100rpx;
  line-height: 100rpx;
}

/* goods */

.goods-list {
  display: flex;
  flex-wrap: wrap;
}

.goods-list .goods {
  flex: 50% 0 0;
  position: relative;
}

.goods-list .goods image {
  display: block;
}

/* no-content */

.no-content-image {
  margin-top: 20%;
  text-align: center;
}

.no-content-image image {
  display: block;
  width: 18%;
  margin: 0 auto;
}

/* 售罄 */

.no-stock {
  position: absolute;
  top: 40%;
  left: 50%;
  width: 30%;
  transform: translate(-50%, -50%);
}
