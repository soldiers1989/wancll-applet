page{
  width: 100%;
  height: 100%;
}
.content{
  width: 100%;
  height: 100%;
  position: relative
}
.content .bg-image{
  width: 100%;
  height: 100%;
}
.ewm-box{
  width: 100%;
  height: 76%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.ewm-box .ewm-border{
  width: 170px;
  height: 170px;
  background: #fff;
  border-radius: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
}
