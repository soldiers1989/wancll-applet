.banner_swiper {
  width: 100%;
  height: 188px;
}

.banner_swiper image {
  width: 100%;
}

.banner-list {
  display: flex;
  padding: 30rpx 0;
  text-align: center;
}

.banner-list .list {
  flex: 1;
}

.banner-list image {
  width: 88rpx;
}
.coupon .title{
  padding:10px;
}
.lists{
  display: flex;
  flex-wrap: wrap;
  padding: 10rpx;
}
.lists .list{
  flex: 50% 0 0;
}
.lists .list .coupon{
  margin: 10rpx;
  background: #fff;
  padding: 10px;
}