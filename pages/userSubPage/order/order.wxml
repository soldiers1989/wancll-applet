<!--pages/userSubPage/order/order.wxml-->
<view>
  <zan-tab 
  id="tab"
  class="tab"
  bindtap="tabchange" 
  list="{{ tabList }}" 
  selected-id="{{ tabSelectedId }}" 
  data-target="{{ tabSelectedId }}"
  fixed
  >
    <block wx:for="{{orderList}}" wx:key="key">
      <order-item 
        data="{{item}}" 
        bind:refreshGet="refreshGet"
      ></order-item>
    </block>
  
  </zan-tab>
  
</view>