.conten{
  padding-bottom: 100rpx;
}
.convert{
  display: flex;
  background: #fff;
  padding: 10px;
}
.convert .convert-input{
  background: #f5f5f5;
  border-radius: 2px;
  line-height:100rpx; 
  flex: 1;
}
.convert .convert-btn{
  flex: 150rpx 0 0;
  margin-left: 20px;
}
.discount-list{
  padding: 10px;
}
.discount-list .discount{
  padding: 10px;
  background: #fff;
  height: 130rpx;
  margin-bottom: 10px;
  border-radius: 10px;
  display: flex
}
.discount-list .discount .left{
  flex: 150rpx 0 0;
}
.discount-list .discount .center{
  flex: 1;
}
.discount-list .discount .right{
  flex: 150rpx 0 0;
}
.fix-bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}