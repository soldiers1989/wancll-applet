.log-box{
  text-align: center;
}
.logo{
  margin-top: 100rpx;
  margin-bottom: 50rpx;
  margin-left: 150rpx;
}
.iconfont{
  font-size: 65rpx;
  color: #09bb07;
}
.panle{
  display: flex;
  padding: 10px;
}
.panle input{
  flex: 1;
  padding-left: 15px;
  background: #fff;
  height: 90rpx;
}
.panle .right{
  margin-left: 10px;
  flex: 250rpx 0 0;
}
.greet{
  color:#999;
  text-align: center;
  line-height: 50px;
  font-size: 28rpx;
}
.greet text{
  color: #000;
  text-decoration: underline;
  font-weight: bold;
}

.button {
  background-color:#ff0000;
  letter-spacing: 0.2rem;
}

.panle .right{
  margin-left: 10px;
  flex: 250rpx 0 0;
  font-size:0rpx;
}
.panle .right button{
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #fff;
}